  `score_setting` varchar(36) DEFAULT NULL,
  `createtime` varchar(36) DEFAULT NULL,
  `position_source` int(11) DEFAULT NULL,
  `auto_forward_emails` varchar(255) DEFAULT NULL,
  `dontsendemail` int(11) DEFAULT NULL,
  `total_resume_count` int(11) DEFAULT NULL,
  `type` varchar(25) DEFAULT NULL,
  `workage` int(11) DEFAULT NULL,
  `degree` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=416 DEFAULT CHARSET=utf8



  `first_language_level` int(11) DEFAULT NULL,
  `second_language` varchar(200) DEFAULT NULL,
  `second_language_level` int(11) DEFAULT NULL,
  `sex` int(11) DEFAULT NULL,
  `constellation` varchar(64) DEFAULT NULL,
  `latitude` varchar(12) DEFAULT NULL,
  `longitude` varchar(12) DEFAULT NULL,
  `citynum` varchar(9) DEFAULT NULL,
  `worklocation` varchar(196) DEFAULT NULL,
  `workingtime` varchar(111) DEFAULT NULL,
  `publish_time` varchar(36) DEFAULT NULL,
  `closing_time` varchar(36) DEFAULT NULL,
  `admin_user_id` int(11) DEFAULT NULL,
  `picname` varchar(50) DEFAULT NULL,
  `state` int(11) DEFAULT NULL,
  `autoqa` int(11) DEFAULT NULL,
  `bole_user_id` int(11) DEFAULT NULL,
  `position_type` varchar(50) DEFAULT NULL,
  `total_match_count` int(11) DEFAULT NULL,
  `school` int(11) DEFAULT NULL,
  `education` int(11) DEFAULT NULL,
  `position_score` int(11) DEFAULT NULL,
  `workproperty` varchar(16) DEFAULT NULL,
  `description` text,
  `position_type1` varchar(50) DEFAULT NULL,
  `position_mode` int(11) DEFAULT NULL,
  `jiazhiguan` varchar(255) DEFAULT NULL,
  `zhiyexingge` varchar(255) DEFAULT NULL,
  `zhiyexingqu` varchar(255) DEFAULT NULL,
  `basic_score` varchar(12) DEFAULT NULL,

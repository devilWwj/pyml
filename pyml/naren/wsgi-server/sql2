CREATE TABLE `worktest` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `wage` varchar(128) DEFAULT NULL,
  `unit_name` text,
  `scale` int(11) DEFAULT NULL,
  `description` text,
  `position_num` int(11) DEFAULT NULL,
  `start_time` varchar(20) DEFAULT NULL,
  `end_time` varchar(20) DEFAULT NULL,
  `trade_num` varchar(12) DEFAULT NULL,
  `trade` varchar(128) DEFAULT NULL,
  `position_name` varchar(128) DEFAULT NULL,
  `unittype` int(11) DEFAULT NULL,
  `department` varchar(128) DEFAULT NULL,
  `otherfield` varchar(255) DEFAULT NULL,
  `experience_id` int(11) DEFAULT NULL,
  `resume_id` varchar(24) DEFAULT NULL,
  `last_time` varchar(37) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `resume_id` (`resume_id`)
) ENGINE=InnoDB AUTO_INCREMENT=27587 DEFAULT CHARSET=utf8


CREATE TABLE `educationtest` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `major` varchar(128) DEFAULT NULL,
  `schoolid` int(11) DEFAULT NULL,
  `description` text,
  `degree` varchar(128) DEFAULT NULL,
  `schoolname` varchar(128) DEFAULT NULL,
  `start_time` varchar(20) DEFAULT NULL,
  `end_time` varchar(20) DEFAULT NULL,
  `resume_id` varchar(24) DEFAULT NULL,
  `education_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `resume_id` (`resume_id`)
) ENGINE=InnoDB AUTO_INCREMENT=12570 DEFAULT CHARSET=utf8
